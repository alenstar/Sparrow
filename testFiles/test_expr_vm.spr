//该测试文件用来测试语言的基本功能
//如三种基本类型、以及一些运算符

def main() {

//该文件正常的输出
printLine("==========")
printLine("【虚拟机】测试基本表达式")

//整型
int_value = 10
if int_value != 10 {
  printLine("failed 1")
  printLine(int_value)
} else {
  printLine("success 1")  
}

//浮点型
float_value = 2.2
if float_value != 2.2 {
  printLine("failed 2")
  printLine(float_value)
} else {
  printLine("success 2")  
}

float_value2 = 22
if float_value2 != 22 {
  printLine("failed 3")
  printLine(float_value2)
} else {
  printLine("success 3")  
}


//字符串
str_value = ""
if (str_value != "") {
  printLine("failed 4")  
  printLine(str_value)
} else {
  printLine("success 4")  
}

str_value2 = "z"
if (str_value2 != "z") {
  printLine("failed 5")  
  printLine(str_value2)
} else {
  printLine("success 5")  
}

str_value3 = "sparrow language"
if (str_value3 != "sparrow language") {
  printLine("failed 6")  
  printLine(str_value3)
} else {
  printLine("success 6")  
}

//运算符
add = int_value + 2
if add != 12 {
  printLine("failed 7")
  printLine(add)
} else {
  printLine("success 7")  
}

add = float_value + 1.1
if add != 3.3 {
  printLine("failed 8")
  printLine(add)
} else {
  printLine("success 8")  
}

add = int_value + float_value
if add != 12.2 {
  printLine("failed 9")
  printLine(add)
} else {
  printLine("success 9")  
}

minus = int_value - 2
if minus != 8 {
  printLine("failed 10")
  printLine(minus)
} else {
  printLine("success 10")  
}

minus = float_value - 1.1
if minus != 1.1 {
  printLine("failed 11")
  printLine(minus)
} else {
  printLine("success 11")  
}

minus = int_value - float_value
if minus != 7.8 {
  printLine("failed 12")
  printLine(minus)
} else {
  printLine("success 12")  
}

multi = int_value * 3
if multi != 30 {
  printLine("failed 13")
  printLine(multi)
} else {
  printLine("success 13")  
}

multi = float_value * 3
if multi != 6.6 {
  printLine("failed 14")
  printLine(multi)
} else {
  printLine("success 14")  
}

multi = int_value * float_value
if multi != 22 {
  printLine("failed 15")
  printLine(multi)
} else {
  printLine("success 15")  
}

printLine("==========")

}
