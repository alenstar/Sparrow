require foo as foo
require bar as bar

require bar as bar

printLine("==========")
printLine("【全局】测试require重复包含")

//重复包含了bar模块
//解释器应该只解析一遍，bar模块中的全局变量值在foo模块中的修改应该要生效

if bar.$bar_value != 20 {
  printLine("failed 1")  
  printLine(bar.$bar_value)
} else {
  printLine("success 1")  
}

if bar.$bar_str != "foo_bar" {
  printLine("failed 2")  
  printLine(bar.$bar_str)
} else {
  printLine("success 2")  
}

if bar.$foo != "new value" {
  printLine("failed 3")  
  printLine(bar.$foo)
} else {
  printLine("success 3")  
}

printLine("==========")

