print("test function")
def fact(n) {
  f = 1
  while n > 0 {
    f = f * n
    n = n - 1  
  }
  return f
}
result = fact(3)
print("6 succes")

print("test fib")
def fib(n) {
  if n < 2 {
    return n  
  } 
  else {
    return fib(n - 1) + fib(n - 2) 
  }
}
result = fib(10)
print("55 success")

